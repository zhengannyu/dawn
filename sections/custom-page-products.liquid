{% style %}
  .page-title {
    margin: 0 0 40px;
  }
{% endstyle %}
{% assign photos = shop.metaobjects.products.values %}

<div class="page-width flex-col">
  {% for photo in photos %}
    <img
      class="slide"
      srcset="{{ photo.photo | image_url: width: 800 }}"
      sizes="(min-width: 400px) 298px, 40vw"
      alt=""
      width="100%"
      height="auto">
  {% endfor %}
</div>
{% comment %} <iframe
  class="product-pdf"
  src="https://drive.google.com/file/d/1ovebimp7TM0vtaWhpgQavR2r8uFFKDTo/preview"
  width="100%"
  height="100vh">
</iframe> {% endcomment %}